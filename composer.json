{
  "name": "mbvb1223/chat-ai-multiclient",
  "description": "A PHP package to interact with OpenAI, Gemini, and Grok chat APIs",
  "type": "library",
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "openai-php/client": "^0.10.3",
    "gemini-api-php/client": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "friendsofphp/php-cs-fixer": "^3.70"
  },
  "autoload": {
    "psr-4": {
      "Mbvb1223\\ChatAiMulticlient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mbvb1223\\ChatAiMulticlient\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix": "vendor/bin/php-cs-fixer fix . -vvv",
    "check": "vendor/bin/php-cs-fixer check . -vvv",
    "test": "vendor/bin/phpunit --testdox"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Khien",
      "email": "phamkhien2309@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
